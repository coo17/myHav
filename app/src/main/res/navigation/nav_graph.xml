<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.cleo.myha.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/habitFragment"
        android:name="com.cleo.myha.habits.HabitFragment"
        android:label="fragment_habit"
        tools:layout="@layout/fragment_habit" />
    <fragment
        android:id="@+id/discoverFragment"
        android:name="com.cleo.myha.discover.DiscoverFragment"
        android:label="fragment_discover"
        tools:layout="@layout/fragment_discover" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.cleo.myha.profile.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" /><action android:id="@+id/action_global_homeFragment" app:destination="@id/homeFragment"/><action android:id="@+id/action_global_habitFragment" app:destination="@id/habitFragment"/><action android:id="@+id/action_global_discoverFragment" app:destination="@id/discoverFragment"/><action android:id="@+id/action_global_profileFragment" app:destination="@id/profileFragment"/>
    <fragment
        android:id="@+id/createHabitFragment"
        android:name="com.cleo.myha.createhabits.CreateHabitFragment"
        android:label="fragment_create_habit"
        tools:layout="@layout/fragment_create_habit" >
        <argument
            android:name="reminder"
            app:argType="integer"
            android:defaultValue="-1" />
        <argument
            android:name="minutes"
            app:argType="integer"
            android:defaultValue="-1" />
    </fragment>
    <action android:id="@+id/action_global_createHabitFragment" app:destination="@id/createHabitFragment" />
    <fragment
        android:id="@+id/timePickerDialog"
        android:name="com.cleo.myha.createhabits.TimePickerDialog"
        android:label="dialog_timer_picker"
        tools:layout="@layout/dialog_timer_picker" />
    <action android:id="@+id/action_global_timePickerDialog" app:destination="@id/timePickerDialog" />
    <fragment
        android:id="@+id/publishFragment"
        android:name="com.cleo.myha.PublishFragment"
        android:label="fragment_publish"
        tools:layout="@layout/fragment_publish" />
    <action android:id="@+id/action_global_publishFragment" app:destination="@id/publishFragment" />
    <fragment
        android:id="@+id/chatRoomsFragment"
        android:name="com.cleo.myha.chatroom.ChatRoomsFragment"
        android:label="fragment_chat_rooms"
        tools:layout="@layout/fragment_chat_rooms" /><action android:id="@+id/action_global_chatRoomsFragment" app:destination="@id/chatRoomsFragment"/>

</navigation>